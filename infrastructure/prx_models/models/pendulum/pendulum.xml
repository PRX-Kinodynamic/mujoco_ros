<mujoco model="mushr_nano">
  <compiler angle="radian" />
  <size njmax="500" nconmax="100"/>
  <option timestep="0.002" integrator="Euler"/>
  <option>
      <flag multiccd="enable"/>
  </option>
  <include file="../friction_floors/friction_floor_uniform.xml"/>
  <visual>
    <headlight ambient="0.6 0.6 0.6" diffuse="0.5 0.5 0.5" specular="0.2 0.2 0.2"/>
    <map znear="0.001" />
  </visual>
  <worldbody>
    <body name="fix_box" pos="0.0 0.0 2" euler="0 0 0.0">
        <geom  type="box"  name="fix_box" contype="1" conaffinity="1"  condim="6" 
               size="0.1 0.1 0.1"  pos="0 0 0" euler="0 0 0"  rgba="0.5 0 0 1.0"/>
        <body name="pendulum" pos="0.2 0.0 -0.5">
            <joint name="joint0" type="hinge" axis="1 0 0" limited="false" pos="0.0 0.0 0.5"
                   frictionloss="0.01" damping="0.1" armature="0.0002"  />
            <geom name="pendulum" type="box" contype="1" conaffinity="1" mass="0.5" 
                  size="0.1 0.1 0.5"  rgba="1 0 0 1.0"/>
        </body>
    </body>
  </worldbody>
    <actuator>
    <motor gear="1" forcelimited="true" forcerange="-10 10" name="joint0_motor" joint="joint0"/>
  </actuator>
</mujoco>
